{% extends "base.jinja" %}

{% block content %}

    <h2>Sign In</h2>
    {{ form.hidden_tag() }}
    <p>
        {{ form.email.label }}<br>
        {{ form.email(size=32) }}

    </p>
    <p>
        {{ form.password.label }}<br>
        {{ form.password(size=32) }}
    </p>
    <p>
        {{ form.remember_me()}} {{ form.remember_me.label }}
    </p>
    <p>{{ form.submit() }}</p>

{% endblock  %}

